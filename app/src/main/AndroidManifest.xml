<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.easyapp.customcs">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_claro_shop"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_claro_shop_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CustomCS">
        <activity
            android:name=".Splash"
            android:exported="true"
            android:theme="@style/Theme.CustomCS.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/claroShop"
            android:theme="@style/Theme.CustomCS.NoActionBar" />
    </application>

</manifest>