<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.CustomCS" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/primaryLightColor</item>
        <item name="colorPrimaryVariant">@color/primaryDarkColor</item>
        <item name="colorOnPrimary">@color/primaryTextColor</item>

        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/secondaryLightColor</item>
        <item name="colorSecondaryVariant">@color/secondaryDarkColor</item>
        <item name="colorOnSecondary">@color/secondaryTextColor</item>

        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorSecondary</item>
        <!-- Customize your theme here. -->
    </style>

    <!--NoActionBar, definida en manifest-->
    <style name="Theme.CustomCS.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <!--appbar-->
    <style name="Theme.DisenioCS.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <!--toolbar-->
    <style name="Theme.DisenioCS.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources><!--
